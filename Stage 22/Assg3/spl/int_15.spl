alias taskManager R0;
taskManager = SYSTEM_STATUS_TABLE + 24;
print "contextSwitches: ";
print [taskManager];
print "------------";
print "maxMemUsage: ";
print [taskManager + 1];
print "------------";
print "kernelSwitches: ";
[taskManager + 3] = [taskManager + 3] + 1;
print [taskManager + 3];
halt;
